<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activity.AdminMainActivity"
    tools:showIn="@layout/app_bar_admin_main">

    <Button
        android:id="@+id/btCadastrarPonto"
        android:layout_width="302dp"
        android:layout_height="55dp"
        android:text="@string/btCadastrarPonto"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/btListar"
        android:layout_width="302dp"
        android:layout_height="55dp"
        android:text="@string/btListar"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="243dp" />

    <Button
        android:id="@+id/btCadastrarLocal"
        android:layout_width="302dp"
        android:layout_height="55dp"
        android:text="@string/btCadastrarLocal"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="98dp" />

    <Button
        android:id="@+id/btCadastrarSala"
        android:layout_width="302dp"
        android:layout_height="55dp"
        android:text="@string/btCadastrarSala"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="173dp" />

</android.support.constraint.ConstraintLayout>